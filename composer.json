{
	"name" : "stinger-soft/entity-search-bundle",
	"type" : "symfony-bundle",
	"description" : "Abstraction Layer to index and search entities",
	"keywords" : [
		"Search"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\EntitySearchBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : "^8.1",
		"ext-json": "*",
		"doctrine/annotations": "^2",
		"doctrine/dbal": "^4",
		"doctrine/event-manager": "^2",
		"doctrine/persistence": "^3",
		"doctrine/orm": "^3",
		"knplabs/knp-components" : "^v5",
		"symfony/config": "^6.4",
		"symfony/console" : "^6.4",
		"symfony/dependency-injection" : "^6.4",
		"symfony/event-dispatcher-contracts": "^3.0",
		"symfony/form": "^6.4",
		"symfony/framework-bundle": "^6.4",
		"symfony/http-foundation": "^6.4",
		"symfony/http-kernel" : "^6.4",
		"symfony/options-resolver": "^6.4",
		"symfony/property-access" : "^6.4",
		"stinger-soft/php-commons" : "^1.0.0"
	},
	"require-dev" : {
		"symfony/validator" : "^6.4",
		"phpunit/phpunit" : "^9.5",
		"php-coveralls/php-coveralls" : "^2.5",
		"symfony/phpunit-bridge" : "^6.4"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.2.x-dev",
			"dev-develop" : "2.0.x-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"ocramius/package-versions": false
		}
	}
}
